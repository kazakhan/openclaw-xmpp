  "type": "module",
  "main": "index.ts",
  "bin": {
    "openclaw-xmpp": "./cli-standalone.js"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
